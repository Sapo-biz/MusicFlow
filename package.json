{
  "name": "musicflow",
  "version": "1.0.0",
  "description": "Professional Music Creator Platform - A comprehensive web-based music production suite",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "serve": "npx http-server -p 8000 -c-1",
    "dev": "npx live-server --port=8000 --open=/index.html",
    "build": "echo 'No build process needed - pure HTML/CSS/JS'",
    "deploy": "netlify deploy --prod --dir=public",
    "deploy:preview": "netlify deploy --dir=public",
    "test": "echo 'No tests specified'",
    "lint": "echo 'No linter configured'"
  },
  "keywords": [
    "music",
    "audio",
    "synthesis",
    "sequencer",
    "web-audio",
    "music-production",
    "daw",
    "instruments",
    "effects",
    "creative"
  ],
  "author": "MusicFlow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/musicflow.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/musicflow/issues"
  },
  "homepage": "https://your-username.github.io/musicflow",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "netlify-cli": "^17.0.0"
  },
  "browserslist": [
    "last 2 versions",
